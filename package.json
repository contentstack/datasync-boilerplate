{
  "name": "contentstack-sync-boilerplate",
  "version": "1.0.0",
  "description": "Contentstack sync utility boilerplate",
  "main": "index.js",
  "scripts": {
    "continue": "node dist/index",
    "start": "npm run build && node dist/index",
    "build": "npm run clean && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "clean": "rimraf dist/ .ledger .token .checkpoint unprocessible/",
    "node-debugger": "npm run build && node --inspect dist/index"
  },
  "dependencies": {
    "contentstack-asset-store-filesystem": "file:modules/contentstack-asset-store-filesystem-1.0.0.tgz",
    "contentstack-content-store-filesystem": "file:modules/contentstack-content-store-filesystem-1.0.0.tgz",
    "contentstack-content-store-mongodb": "file:./modules/contentstack-content-store-mongodb-1.0.0.tgz",
    "contentstack-sync-manager": "file:modules/contentstack-sync-manager-1.0.0.tgz",
    "contentstack-webhook-listener": "file:./modules/contentstack-webhook-listener-1.0.0.tgz"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.14.1",
    "rimraf": "^2.6.2"
  },
  "author": "Contentstack Ecosystem",
  "license": "ISC"
}
