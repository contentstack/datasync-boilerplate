{
  "name": "datasync-boilerplate",
  "version": "0.0.1-rc.1",
  "description": "A boilerplate that has the code skeleton to help you get up and running with Contentstack DataSync.",
  "main": "index.js",
  "scripts": {
    "continue": "node dist/index",
    "start": "npm run build && node dist/index",
    "build": "babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "clean": "rimraf dist/ .ledger .token .checkpoint unprocessible/",
    "node-debugger": "npm run build && node --inspect dist/index"
  },
  "dependencies": {
    "@contentstack/datasync-asset-store-filesystem": "0.0.1-rc.1",
    "@contentstack/datasync-content-store-filesystem": "0.0.1-rc.1",
    "@contentstack/datasync-content-store-mongodb": "0.0.1-rc.1",
    "@contentstack/datasync-manager": "0.0.1-rc.1",
    "@contentstack/webhook-listener": "0.0.1-rc.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.14.1",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentstack/datasync-boilerplate"
  },
  "author": "Contentstack LLC <support@contentstack.com>",
  "license": "MIT"
}
